year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{RVS}: Robust Variant Score Test for NGS data with external controls",
         author = c(person("Andriy", "Derkach"),
                    person("Ted", "Chiang"),
                    person("Jiafen", "Gong"),
                    person("Lisa", "Strug")),
         year = year,
         note = note,
         url = "https://CRAN.R-project.org/package=RVS")
